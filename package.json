{
  "name": "@unplugin-unocss-config/monorepo",
  "type": "module",
  "version": "0.0.6",
  "private": true,
  "packageManager": "pnpm@8.12.0",
  "scripts": {
    "dev": "pnpm -r --filter=./packages/* --parallel run dev",
    "build": "pnpm -r --filter=./packages/* --parallel run build",
    "lint": "eslint .",
    "play": "pnpm -C playground run dev",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish"
  },
  "dependencies": {
    "unplugin": "^1.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.6",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.5",
    "@unocss/config": "^0.58.0",
    "@unocss/core": "^0.58.0",
    "@unocss/preset-mini": "^0.58.0",
    "bumpp": "^9.2.1",
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.1",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "webpack": "^5.89.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
