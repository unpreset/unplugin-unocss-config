{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "jsx": "preserve",
    "lib": ["esnext", "dom"],
    "module": "esnext",
    "resolveJsonModule": true,
    "types": [
      "vite/client"
    ],
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true
  },
  "exclude": [
    "**/dist/**",
    "**/node_modules/**"
  ]
}
